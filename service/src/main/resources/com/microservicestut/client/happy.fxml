<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.chart.BarChart?>
<VBox xmlns:fx="http://javafx.com/fxml"
      spacing="10" alignment="CENTER"
      fx:controller="com.microservicestut.client.mood.HappinessController">
    <BarChart fx:id="happinessOverTime" title="Happiness Over Time">
        <xAxis><CategoryAxis label="Time"/></xAxis>
        <yAxis><NumberAxis fx:id="yAxis" label="Count"/></yAxis>
    </BarChart>
</VBox>